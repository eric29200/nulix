VERSION="2.30"
SRC_FILENAME="links-"$VERSION".tar.bz2"
URL="http://links.twibright.com/download/"$SRC_FILENAME

function configure_port() {
	./configure								\
		--host=$HOST							\
		--prefix="/usr"
}

function build_port() {
	make -j$NJOBS
}

function install_port() {
	make install DESTDIR=$SYSROOT
}
